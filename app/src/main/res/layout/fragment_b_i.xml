<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BIFragment">

    <!-- TODO: Update blank fragment layout -->


    <LinearLayout
        android:layout_margin="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:outlineSpotShadowColor="@color/purple_200">

        <EditText
            android:id="@+id/search_text_bi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:outlineSpotShadowColor="@color/purple_200"
            android:hint="Masukan kata disini" />


        <TextView
            android:id="@+id/kontent_text1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Daftar Kata Yang Mungkin Anda Cari" />



        <ListView
            android:id="@+id/receyler"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </LinearLayout>
</FrameLayout>